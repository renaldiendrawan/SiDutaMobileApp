        android:label="SiDuta"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/logoaplikasi"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiEDuta"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
